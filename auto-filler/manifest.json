{
  "manifest_version": 2,
  "name": "Auto filler",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "<all_urls>"
    ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js", "formfiller.js"]
    }
    
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}

